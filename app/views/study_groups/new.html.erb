<h2 style="border-bottom: 1px solid;"> Create Study Group </h2>
<% if @study_group.errors.full_messages.present? %>
  <% @study_group.errors.full_messages.each do |message| %>
    <ul>
      <li><%= message %></li>
    </ul>
  <% end %>
<% end %>
<%= form_with(model: @study_group, local: true) do |f| %>
  <p>
  <%= f.label :name %>
  <%= f.text_field :name %>
  </p>  
  
  <%= f.submit %>
<% end %> 